{
  "name": "@sonatype/react-shared-components-gallery",
  "version": "14.0.0",
  "description": "Gallery application to demonstrate usage and look of Sonatype shared UI components",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm-run-all lint webpack-dev-server",
    "build": "npm-run-all ci-lint webpack copy-assets",
    "copy-assets": "cpx \"{src/index.html,src/assets/images/favicon*}\" dist",
    "lint": "eslint \"./src/**/*.{js,ts}{,x}\"",
    "ci-lint": "eslint \"./src/**/*.{js,ts}{,x}\" --max-warnings=0",
    "lint-tests": "eslint \"visualtests/*.js\"",
    "ci-lint-tests": "eslint \"visualtests/*.js\" --max-warnings=0",
    "webpack": "webpack --env production --mode=production",
    "webpack-dev-server": "webpack serve",
    "test": "npm-run-all check-install lint-tests build jest jest-dark",
    "ci-test": "npm-run-all check-install ci-lint-tests build ci-jest ci-jest-dark",
    "check-install": "yarn install --check-files --frozen-lockfile",
    "ci-jest": "jest --ci --testTimeout=120000",
    "ci-jest-dark": "RSC_GALLERY_THEME=DARK jest --ci --testTimeout=120000",
    "jest": "jest --testTimeout=120000",
    "jest-dark": "RSC_GALLERY_THEME=DARK jest --testTimeout=120000",
    "clean": "rimraf dist webpack-modules test-results"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype/sonatype-react-shared-components.git",
    "directory": "gallery"
  },
  "author": "Sonatype, Inc.",
  "license": "EPL-2.0",
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.1",
    "@babel/runtime": "^7.26.7",
    "@types/classnames": "^2.3.4",
    "@types/debounce": "^1.2.4",
    "@types/ramda": "^0.30.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "copy-modules-webpack-plugin": "^2.2.1",
    "cpx": "^1.5.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.20.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "7.37.4",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "jest-junit": "^16.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^24.2.0",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "source-map-loader": "^5.0.0",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "typescript": "~5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@nivo/bar": "^0.88.0",
    "@nivo/core": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@nivo/pie": "^0.88.0",
    "@nivo/tooltip": "^0.88.0",
    "@react-hook/debounce": "^4.0.0",
    "@sonatype/react-shared-components": "link:../lib/dist",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "compare-versions": "^6.1.1",
    "core-js": "^3.40.0",
    "es6-object-assign": "^1.1.0",
    "es6-set": "^0.1.6",
    "prop-types": "^15.8.1",
    "query-string": "^9.1.1",
    "ramda": "^0.30.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "react-syntax-highlighter": "^15.6.1"
  }
}
