    "webpack": "webpack --env.production",
    "webpack-dev-server": "webpack-dev-server",
    "test": "wdio run wdio.conf.js",
    "clean": "rimraf dist webpack-modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype/sonatype-react-shared-components.git",
    "directory": "gallery"
  },
  "author": "Sonatype, Inc.",
  "license": "EPL-2.0",
