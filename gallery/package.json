{
  "name": "@sonatype/react-shared-components-gallery",
  "version": "9.20.4",
  "description": "Gallery application to demonstrate usage and look of Sonatype shared UI components",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm-run-all lint webpack-dev-server",
    "build": "npm-run-all lint webpack copy-assets copy-hosted-versions",
    "copy-assets": "npm-run-all cpx \"{src/index.html,src/assets/images/favicon*}\" dist",
    "copy-hosted-versions": "cpx hosted-versions.json dist",
    "lint": "eslint 'src/**/*.{js,ts}{,x}'",
    "webpack": "webpack --env production --mode=production",
    "webpack-dev-server": "webpack serve",
    "test": "npm-run-all check-install build jest",
    "check-install": "yarn install --check-files --frozen-lockfile",
    "jest": "jest --testTimeout=120000",
    "clean": "rimraf dist webpack-modules test-results"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype/sonatype-react-shared-components.git",
    "directory": "gallery"
  },
  "author": "Sonatype, Inc.",
  "license": "EPL-2.0",
  "devDependencies": {
    "@axe-core/puppeteer": "^4.3.2",
    "@babel/runtime": "^7.12.0",
    "@types/classnames": "^2.2.11",
    "@types/debounce": "^1.2.0",
    "@types/ramda": "^0.27.64",
    "@types/react": "^16.14.14",
    "@types/react-dom": "^16.9.14",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "chromedriver": "^88.0.0",
    "copy-modules-webpack-plugin": "^2.2.0",
    "cpx": "^1.5.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.19.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "jest": "^27.2.4",
    "jest-image-snapshot": "^4.5.1",
    "jest-junit": "^12.3.0",
    "mini-css-extract-plugin": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^10.4.0",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.37.5",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^2.0.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.20.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@react-hook/debounce": "^4.0.0",
    "@sonatype/react-shared-components": "link:../lib/dist",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "core-js": "^3.8.3",
    "es6-object-assign": "^1.1.0",
    "es6-set": "^0.1.5",
    "query-string": "^6.13.8",
    "ramda": "^0.28.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3"
  }
}
